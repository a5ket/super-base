{
    "name": "super-base",
    "version": "1.0.0",
    "private": true,
    "description": "Superhero database monorepo",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "pnpm -r dev",
        "build": "pnpm -r build",
        "start:backend": "pnpm --filter backend start",
        "start:frontend": "pnpm --filter frontend preview"
    },
    "engines": {
        "node": ">=18",
        "pnpm": ">=8"
    }
}